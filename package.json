    "@dnd-kit/modifiers": "^3.0.0",
    "@dnd-kit/sortable": "^4.0.0",
    "llmsWPData": "npm:@wordpress/data@^5.1.2",
    "react-select": "^4.1.0",
    "throttle-debounce": "^2.0.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@dnd-kit/utilities": "^3.2.1",
    "@lifterlms/dev": "^0.0.4-alpha.0",
    "@lifterlms/llms-e2e-test-utils": "^4.0.0",
    "@lifterlms/scripts": "^3.1.0",
    "@wordpress/block-editor": "^11.4.0",
    "@wordpress/blocks": "^12.4.0",
    "@wordpress/components": "^23.4.0",
    "@wordpress/data": "^8.4.0",
    "@wordpress/edit-post": "^7.4.0",
    "@wordpress/editor": "^13.4.0",
    "@wordpress/eslint-plugin": "^14.0.0",
    "@wordpress/notices": "^3.27.0",
    "@wordpress/server-side-render": "^4.4.0",
    "caniuse-lite": "^1.0.30001457",
